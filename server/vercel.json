{
    "version": 2,
    "name": "delivery-app",
    "builds": [
      {
        "src": "/server.js",
        "use": "@vercel/node"
      },
      {
        "src": "/build/index.html",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/server.js"
      },
      {
        "src": "/(.*)",
        "dest": "/build/index.html"
      }
    ]
  }